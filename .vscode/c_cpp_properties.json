    "version": 4,
    "configurations": [
        {
            "name": "Arch_linux",
            "includePath": [
                "${workspaceFolder}/dev/",
                "${workspaceFolder}/lwmem/src/include/"
            ],
            "defines": [
                "WIN32",
                "_DEBUG",
                "UNICODE",
